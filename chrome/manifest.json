{
    "manifest_version": 2,
    "name": "Job App Auto Fill",
    "short_name": "Job Auto Fill",
    "version": "1.0.0",
    "description": "Automatically fills out job application forms based on input form IDs",
    "browser_action": {
      "default_title": "Auto Fill Settings",
      "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ]
  }